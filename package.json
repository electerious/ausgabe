{
  "name": "ausgabe",
  "version": "1.0.0",
  "description": "Tiny logger with zero defaults",
  "keywords": [
    "log",
    "logger",
    "logging",
    "debug",
    "debugging",
    "console"
  ],
  "homepage": "https://github.com/electerious/ausgabe",
  "repository": {
    "type": "git",
    "url": "https://github.com/electerious/ausgabe.git"
  },
  "license": "MIT",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src"
  ],
  "scripts": {
    "eslint": "eslint \"**/*.js\"",
    "format": "npm run eslint -- --fix && npm run prettier -- --write",
    "lint": "npm run eslint && npm run prettier -- --check",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,json,md,yml}\"",
    "test": "npm run lint && node --test",
    "test:coverage": "npm run lint && npm run prettier -- --check && c8 --all --include \"src/**/*.js\" --reporter=lcov --reporter=text-summary ava"
  },
  "dependencies": {
    "chalk": "^5.1.2"
  },
  "devDependencies": {
    "@electerious/eslint-config": "^5.2.0",
    "@electerious/prettier-config": "^4.0.0",
    "ava": "^5.0.1",
    "c8": "^7.12.0",
    "eslint-config-prettier": "^8.5.0"
  },
  "authors": [
    "Tobias Reich <tobias@electerious.com>"
  ]
}
